{
    "name":"dynamic popups",
    
    "version":"1.0.0.0",
    
    "description":"this is a dynamic popup extension",
    
    "manifest_version":2,
    
    "icons":{
        "16":"logos/16x16.png",
        "48":"logos/48x48.png",
        "128":"logos/128x128.png"
    },
    
    "browser_action":{
        "default_title":"dynamic popup",
        "default_popup":"index.html"
    },
    "permissions":["storage"]
}